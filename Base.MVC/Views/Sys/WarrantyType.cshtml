<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <section class="content-header-breadcrumb-miya">
        <div class="row text-right">
            <div class="col-md-12">

                <ol class="breadcrumb breadcrumb-miya">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li><a href="#">UI</a></li>
                    <li class="active">General</li>
                </ol>
            </div>
        </div>
    </section>

    <!-- Main content -->
    <section class="content">

        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-12 connectedSortable">
                <div class="box-body">
                    <div class="nav-tabs-custom">
                        <ul class="nav nav-tabs pull-right">
                            <li class="active"><a href="#tab_1" data-toggle="tab" aria-expanded="true">Warranty Type</a></li>
                            <li class=""><a href="#tab_2" data-toggle="tab" aria-expanded="true" >Warranty Mileage</a></li>
                            <li class="pull-left header"><i class="fa fa-th"></i></li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="tab_1">
                                <div class="box box-info">
                                    <div class="box-header">
                                        <i class="fa fa-comments-o"></i>
                                        <h3 class="box-title">Warranty Name Info</h3>
                                        <form id='wrNameInfoForm' method="get">
                                            <div class="box-body">
                                                <div class="nav-tabs-custom">
                                                    <!-- Warranty Name Başlangıç-->
                                                    <div class="box box-info">
                                                        <div class="box-header">
                                                            <div class="box-tools pull-right">
                                                            </div>
                                                        </div>

                                                        <div class="box-body">
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    <label>Warranty Type Name:</label>
                                                                    <div class="input-group date">
                                                                        <div class="input-group-addon">
                                                                            <i class="fa fa-calendar"></i>
                                                                        </div>
                                                                        <input type="text" class="form-control validate[required]" placeholder="Warranty Type Name" id="txt-wrt_name" required>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                            <div class="col-md-6">
                                                            </div>
                                                            <div class="box-footer clearfix no-border">
                                                                <table class="table text-center">
                                                                    <tbody>
                                                                        <tr>
                                                                            <td width="75%">
                                                                                &nbsp;
                                                                            </td>
                                                                            <td>
                                                                                <button type="reset" class="btn btn-block btn-primary btn-lg" id="btn-cstcp-clear">&nbsp;<i class="fa fa-eraser"></i>&nbsp;&nbsp;Clear&nbsp;&nbsp;</button>
                                                                                @*<button type="reset" class="btn btn-block btn-primary btn-lg" id="btn-cstcp-clear" onclick="resetContactPersonForm();">&nbsp;<i class="fa fa-eraser"></i>&nbsp;&nbsp;Clear&nbsp;&nbsp;</button>*@
                                                                            </td>
                                                                            <td>
                                                                                <button type="submit" class="btn btn-block btn-primary btn-lg" id="btn-cstcp-save" onclick="return insertContactPersonWrapper();">&nbsp;<i class="fa fa-save"></i>&nbsp;&nbsp;Save&nbsp;&nbsp;</button>
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- Warranty Name  son-->

                                                    <div class="box box-info">
                                                        <div class="box-header">
                                                            <i class="fa fa-comments-o"></i>
                                                            <h3 class="box-title">Warranty Name Info</h3>

                                                            <div class="box-tools pull-right">
                                                                <button class="btn bg-teal btn-sm btn-small-miya" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                                            </div>
                                                        </div>
                                                        <div class="box-body" id="loading-image-fxrtInfoGrid">
                                                            <div id="gridContainer_wrName"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            <!-- /.tab-pane -->

                            <div class="tab-pane" id="tab_2">
                                <div class="box box-info">
                                    <div class="box-header">
                                        <i class="fa fa-comments-o"></i>
                                        <h3 class="box-title">Warranty Mileage Info</h3>
                                        <form id='wrMilInfoForm' method="get">
                                            <div class="box-body">
                                                <div class="nav-tabs-custom">

                                                    <!-- Warranty Mileage Başlangıç-->
                                                    <div class="box box-info">
                                                        <div class="box-header">
                                                            <div class="box-tools pull-right">
                                                            </div>
                                                        </div>

                                                        <div class="box-body">
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    <label>Warranty Mileage Name:</label>
                                                                    <div class="input-group date">
                                                                        <div class="input-group-addon">
                                                                            <i class="fa fa-calendar"></i>
                                                                        </div>
                                                                        <input type="text" class="form-control validate[required]" placeholder="Warranty Type Name" id="txt-wrt_mil" required>
                                                                    </div>
                                                                </div>

                                                            </div>
                                                            <div class="col-md-6">
                                                            </div>
                                                            <div class="box-footer clearfix no-border">
                                                                <table class="table text-center">
                                                                    <tbody>
                                                                        <tr>
                                                                            <td width="75%">
                                                                                &nbsp;
                                                                            </td>
                                                                            <td>
                                                                                <button type="reset" class="btn btn-block btn-primary btn-lg" id="btn-cstcp-clear">&nbsp;<i class="fa fa-eraser"></i>&nbsp;&nbsp;Clear&nbsp;&nbsp;</button>
                                                                            </td>
                                                                            <td>
                                                                                <button type="submit" class="btn btn-block btn-primary btn-lg" id="btn-cstcp-save" onclick="return insertContactPersonWrapper();">&nbsp;<i class="fa fa-save"></i>&nbsp;&nbsp;Save&nbsp;&nbsp;</button>
                                                                            </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!-- Warranty Mileage  son-->



                                                    <div class="box box-info">
                                                        <div class="box-header">
                                                            <i class="fa fa-comments-o"></i>
                                                            <h3 class="box-title">Warranty Millage Info</h3>

                                                            <div class="box-tools pull-right">
                                                                <button class="btn bg-teal btn-sm btn-small-miya" data-widget="collapse"><i class="fa fa-minus"></i></button>
                                                            </div>
                                                        </div>
                                                        <div class="box-body" id="loading-image-fxrtInfoGrid">
                                                            <div id="gridContainer_mileage"></div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                            </div>


            </section><!-- /.Left col -->
        </div><!-- /.row (main row) -->


    </section><!-- /.content -->
    <!-- Main content -->
</div><!-- /.content-wrapper -->


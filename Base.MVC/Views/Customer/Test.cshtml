@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Mvc.Localization
@using Base.Core.Entities.Identity
@inject IViewLocalizer Localizer
@*@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet*@

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section stylesPageSpecs {
    <!-- iCheck -->
    <link href="~/adm/plugins/iCheck/minimal/minimal.css" rel="stylesheet" type="text/css" />

    <!-- Date Picker -->
    <link href="~/adm/plugins/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" rel="stylesheet">

    <!-- devex -->
    <link rel="stylesheet" type="text/css" href="~/adm/plugins/devextreme/css/dx.spa.css" />
    <link rel="stylesheet" type="text/css" href="~/adm/plugins/devextreme/css/dx.common.css" />
    <link rel="dx-theme" data-theme="generic.light" href="~/adm/plugins/devextreme/css/dx.light.css" />

    <!-- validationEngine -->
    <link rel="stylesheet" href="~/adm/plugins/validationEngine/validationEngine.jquery.css" type="text/css" />

    <link rel="stylesheet" href="~/adm/dist/css/AdminLTE.css" type="text/css" />
}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <section class="content-header-breadcrumb-miya">
        <div class="row text-right">
            <div class="col-md-12">

                <ol class="breadcrumb breadcrumb-miya">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li><a href="#">UI</a></li>
                    <li class="active">General</li>
                </ol>
            </div>
        </div>
    </section>

    <!-- Main content -->
    <section class="content">

        <!-- Main row -->
        <div class="row">
            <div class="container" id="myWizard">
                <div class="navbar">
                    <div class="navbar-inner">
                        <ul class="nav nav-tabs">
                            <li class="active"><a href="#step1" data-toggle="tabs">Step 1</a></li>
                            <li><a href="#step2" data-toggle="tabs" class="disable" disabled="">Step 2</a></li>
                            <li><a href="#step3" data-toggle="tabs">Step 3</a></li>
                            <li><a href="#step4" data-toggle="tabs">Step 4</a></li>
                        </ul>
                    </div>
                </div>
                <form class="tab-content" method="post" id="myForm">
                    <div class="tab-pane active" id="step1">
                        <p>Here is the content for the first step...</p>
                        <input class="form-control" id="input1" name="input1" required="">
                        <button class="btn btn-default btn-ok" type="button">OK</button>
                    </div>
                    <div class="tab-pane" id="step2">
                        <p>Here is the content for step 2...</p>
                        <input class="form-control" id="input2" name="input2" required="">
                        <button class="btn btn-default btn-ok" type="button">OK</button>
                    </div>
                    <div class="tab-pane" id="step3">
                        <p>Here is the content for step 3...</p>
                        <input class="form-control" id="input3" name="input3" required="">
                        <input class="form-control" id="input4" name="input4" required="">
                        <button class="btn btn-default btn-ok" type="button">OK</button>
                    </div>
                    <div class="tab-pane" id="step4">
                        <p>This is the last step. You're done.</p>
                        <button class="btn btn-default btn-submit" type="submit">Submit</button>
                    </div>
                </form>

                <div class="form-group">
                    <label>Product Interest</label>
                    <div class="">
                        <div id="loading-image-productinterest" class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-copyright"></i>
                            </div>
                            <div id="dropdownProductInterest" class="form-control"></div>
                        </div>
                    </div>
                </div>

                <div class="form-group">
                    <label>MAN Satisfaction</label>
                    <div class="">
                        <div id="loading-image-mansatisfaction" class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-copyright"></i>
                            </div>
                            <div id="dropdownMANSatisfaction" class="form-control"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- /.row (main row) -->


    </section><!-- /.content -->
</div><!-- /.content-wrapper -->


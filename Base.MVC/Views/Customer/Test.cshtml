@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@using Microsoft.AspNetCore.Mvc.Localization
@using Base.Core.Entities.Identity
@inject IViewLocalizer Localizer
@*@inject Microsoft.ApplicationInsights.AspNetCore.JavaScriptSnippet JavaScriptSnippet*@

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@section stylesPageSpecs {
    <!-- iCheck -->
    <link href="~/adm/plugins/iCheck/minimal/minimal.css" rel="stylesheet" type="text/css" />

    <!-- Date Picker -->
    <link href="~/adm/plugins/bootstrap-datepicker/dist/css/bootstrap-datepicker.css" rel="stylesheet">

    <!-- devex -->
    <link rel="stylesheet" type="text/css" href="~/adm/plugins/devextreme/css/dx.spa.css" />
    <link rel="stylesheet" type="text/css" href="~/adm/plugins/devextreme/css/dx.common.css" />
    <link rel="dx-theme" data-theme="generic.light" href="~/adm/plugins/devextreme/css/dx.light.css" />

    <!-- validationEngine -->
    <link rel="stylesheet" href="~/adm/plugins/validationEngine/validationEngine.jquery.css" type="text/css" />

    <link rel="stylesheet" href="~/adm/dist/css/AdminLTE.css" type="text/css" />
    <!-- tagCabin css -->
    <link href="~/adm/plugins/zeyn/tag-cabin/tag-cabin.css" rel="stylesheet" type="text/css" />

}

<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    <!-- Content Header (Page header) -->
    <section class="content-header-breadcrumb-miya">
        <div class="row text-right">
            <div class="col-md-12">

                <ol class="breadcrumb breadcrumb-miya">
                    <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                    <li><a href="#">UI</a></li>
                    <li class="active">General</li>
                </ol>
            </div>
        </div>
    </section>

    <!-- Main content -->
    <section class="content">

        <!-- Main row -->
        <div class="row">

            <div class="form-group">
                <label>Country:</label>

                <div class="">
                    <div id="loading-image-country" class="input-group">
                        <div class="input-group-addon">
                            <i class="fa fa-copyright"></i>
                        </div>
                        <div id="dropdownCountry" class="form-control"></div>
                    </div>
                </div>
            </div>


            <div class="form-group">
                <label>MAN Satisfaction</label>
                <div class="">
                    <div id="loading-image-mansatisfaction" class="input-group">
                        <div class="input-group-addon">
                            <i class="fa fa-copyright"></i>
                        </div>
                        <div id="dropdownMANSatisfaction" class="form-control"></div>
                    </div>
                </div>
            </div>


            <div class="box ">
                <div class="box-header with-border">
                    <h3 class="box-title" lang="en">Services</h3>
                    <div class="box-tools pull-right">
                        <button class="btn btn-box-tool" id="servisDashboardHesapla"><i class="fa fa-refresh"></i></button>
                        <button id="test-collapse" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
                    </div>
                </div><!-- /.box-header -->
                <div class="box-body" style="display: block;">


                    <div class="form-group">
                        <label>Product Interest</label>
                        <div class="">
                            <div id="loading-image-productinterest" class="input-group">
                                <div class="input-group-addon">
                                    <i class="fa fa-copyright"></i>
                                </div>
                                <div id="dropdownProductInterest" class="form-control"></div>
                            </div>
                        </div>
                    </div>
                    <div class="">
                        <div id="loading-image-roles" class="input-group">
                            <div class="input-group-addon">
                                <i class="fa fa-hand-o-right"></i>
                            </div>
                            <div style="margin-bottom: -10px;width:96%" class="tag-container">
                                <ul id="test-cabin" class="tag-box"></ul>
                            </div>
                        </div>

                    </div>

                </div><!-- ./box-body -->
                <div class="box-footer" style="display: block;">

                </div><!-- /.box-footer -->
            </div><!-- /.box -->
        </div><!-- /.row (main row) -->


    </section><!-- /.content -->
</div><!-- /.content-wrapper -->

